{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "types": [
      "node",
      "webpack-env",
      "lodash",
      "jest"
    ],
    "esModuleInterop": true
  },
  "exclude": [
    "test/cypress",
    "/dist",
    ".quasar",
    "node_modules"
  ]
}